    def Save(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        book_id = self.lineEdit_4.text()
        boek_name = self.lineEdit_6.text()
        category = self.lineEdit_32.text()
        aeteur = self.lineEdit_5.text()
        Uitgeverij = self.lineEdit_31.text()
        BoekPrijs = self.lineEdit_7.text()
        OverDitBoek = self.lineEdit_8.text()

        self.cur.execute("INSERT INTO Boek VALUES (?,?,?,?,?,?,?)",
                         (int(book_id), boek_name, category, aeteur, Uitgeverij, int(BoekPrijs), OverDitBoek,))
        self.baglanti.commit()

    def Fix_Search(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        id = self.lineEdit_3.text()
        self.cur.execute("Select * From Boek")
        data = self.cur.fetchall()

        new_data = []

        for i in data:
            for j in i:
                if id == str(j):
                    new_data.append(i)
                    break

        self.lineEdit_9.setText(str(new_data[0][0]))
        self.lineEdit_10.setText(str(new_data[0][1]))
        self.lineEdit_34.setText(str(new_data[0][2]))
        self.lineEdit_11.setText(str(new_data[0][3]))
        self.lineEdit_33.setText(str(new_data[0][4]))
        self.lineEdit_12.setText(str(new_data[0][5]))
        self.lineEdit_13.setText(str(new_data[0][6]))

    def Fix_Delete(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        id = self.lineEdit_3.text()
        self.cur.execute("DELETE FROM Boek WHERE boek_id=?", [int(id)])
        self.baglanti.commit()

    def Fix_Save(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        book_id = self.lineEdit_9.text()
        boek_name = self.lineEdit_10.text()
        category = self.lineEdit_34.text()
        aeteur = self.lineEdit_11.text()
        Uitgeverij = self.lineEdit_33.text()
        BoekPrijs = self.lineEdit_12.text()
        OverDitBoek = self.lineEdit_13.text()

        self.cur.execute(
            "UPDATE Boek SET Boek_Naam=?,Categorie=?,Autheur=?,Uitgeverij=?,Boek_Prijs=?,OverDitBoek=? WHERE Boek_id=?",
            (boek_name, category, aeteur, Uitgeverij, int(BoekPrijs), OverDitBoek, int(book_id)))
        self.baglanti.commit()

    def Add_User(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        user_id = self.lineEdit_35.text()
        user_name = self.lineEdit_26.text()
        mail_adress = self.lineEdit_25.text()
        password = self.lineEdit_24.text()
        password_2 = self.lineEdit_23.text()


        if password != password_2:
            self.statusbar.showMessage("Wachtwoord is niet het zelfde")
        else:
            if self.radioButton_3.isChecked() == True:
                self.cur.execute("INSERT INTO Gebruiker VALUES (?,?,?,?,?)",
                                 (int(user_id), user_name, mail_adress, password, "Gebruiker"))
                self.baglanti.commit()
                self.statusbar.showMessage("Bevoegde personeel succesvol toegevoegd")

                self.lineEdit_35.setText("")
                self.lineEdit_26.setText("")
                self.lineEdit_25.setText("")
                self.lineEdit_24.setText("")
                self.lineEdit_23.setText("")

            elif self.radioButton_4.isChecked() == True:
                self.cur.execute("INSERT INTO Gebruiker VALUES (?,?,?,?,?)",
                                 (int(user_id), user_name, mail_adress, password, "Bevoegde"))
                self.baglanti.commit()
                self.statusbar.showMessage("Gebruiker succesvol toegevoegd")

                self.lineEdit_35.setText("")
                self.lineEdit_26.setText("")
                self.lineEdit_25.setText("")
                self.lineEdit_24.setText("")
                self.lineEdit_23.setText("")

    def Fix_Login(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        user_id=self.lineEdit_19.text()
        password=self.lineEdit_18.text()

        self.cur.execute("Select * from Gebruiker")
        data=self.cur.fetchall()
        #print(data)

        for i in data:
            if user_id==str(i[0]) and password==str(i[3]):
                #print(i)
                self.lineEdit_36.setText(str(i[0]))
                self.lineEdit_17.setText(str(i[1]))
                self.lineEdit_20.setText(str(i[2]))
                self.lineEdit_21.setText(str(i[3]))

    def Fix_Edit(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        user_id=self.lineEdit_36.text()
        user_naam=self.lineEdit_17.text()
        mail_adress=self.lineEdit_20.text()
        password=self.lineEdit_21.text()

        if self.radioButton.isChecked()==True:
            self.cur.execute(
                "UPDATE Gebruiker SET Gebruiker_Naam=?,Email=?,Wachtwoord=?,Type=? WHERE Gebruiker_id=?",(user_naam,mail_adress,int(password),"Gebruiker",int(user_id)))
            self.baglanti.commit()

        elif self.radioButton_2.isChecked()==True:
            self.cur.execute(
                "UPDATE Gebruiker SET Gebruiker_Naam=?,Email=?,Wachtwoord=?,Type=? WHERE Gebruiker_id=?",(user_naam,mail_adress,int(password),"Bevoegde",int(user_id)))
            self.baglanti.commit()




    def Rent_Search(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        self.cur.execute("Select * from Boek")
        data = self.cur.fetchall()

        boek_id=self.lineEdit_30.text()
        new_data=[]
        for i in data:
            for j in i:
                if boek_id==str(j):
                    new_data.append(i)
                    break

        self.lineEdit_29.setText(new_data[0][1])

    def Rent_Rent(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        boek_id = self.lineEdit_30.text()
        user_id=self.lineEdit_28.text()
        rent_date=time.strftime("%Y.%m.%d",time.localtime())

        self.cur.execute("INSERT INTO Huur_Ophalen VALUES(?,?,?,?,?)",(int(boek_id),int(user_id),rent_date,"",0))
        self.baglanti.commit()
        self.statusbar.showMessage("Boek succesvol ingehuurd")
        self.lineEdit_30.setText("")
        self.lineEdit_29.setText("")
        self.lineEdit_28.setText("")

    def Retrieve_Search(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        self.cur.execute("Select * from Boek")
        data = self.cur.fetchall()

        boek_id = self.lineEdit_14.text()
        new_data = []
        for i in data:
            for j in i:
                if boek_id == str(j):
                    new_data.append(i)
                    break

        self.lineEdit_16.setText(new_data[0][1])

    def Retrieve_Retrieve(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        boek_id = self.lineEdit_14.text()
        user_id = self.lineEdit_15.text()
        retrieve_date = time.strftime("%Y.%m.%d",time.localtime())
        print(retrieve_date)

        self.cur.execute("SELECT Huur_datum FROM Huur_Ophalen WHERE Boek_id=? AND Retour_datum=''",(int(boek_id),))
        rent_date=self.cur.fetchall()[0]


        a=date(int(retrieve_date[:4]),int(retrieve_date[5:7]),int(retrieve_date[8:]))
        b=date(int(rent_date[0][:4]),int(rent_date[0][5:7]),int(rent_date[0][8:]))

        day=(a-b).days
        if day>30:
            punishment=(day-30)*1
        else:
            punishment=0

        self.cur.execute("UPDATE Huur_Ophalen SET Retour_datum=?,De_strafzaak=? WHERE Boek_id=? AND Retour_datum=''",(retrieve_date,int(punishment),int(boek_id)))
        self.baglanti.commit()
        self.statusbar.showMessage("Boek succesvol retour")


    def Condition_Search(self):
        self.baglanti = sqlite3.connect("Database.db")
        self.cur = self.baglanti.cursor()

        book_id=self.lineEdit_27.text()

        self.cur.execute("SELECT Boek.Boek_id,Boek.Boek_Naam,Huur_Ophalen.Gebruiker_id,Huur_Ophalen.Huur_datum FROM Boek,Huur_Ophalen "
                         "WHERE Boek.Boek_id=Huur_Ophalen.Boek_id AND Huur_Ophalen.Boek_id=? AND Huur_Ophalen.Retour_datum=''",(int(book_id),))

        data=self.cur.fetchall()
        #print(data)

        retrieve_date = time.strftime("%Y.%m.%d",time.localtime())
        rent_date=data[0][3]

        a = date(int(retrieve_date[:4]), int(retrieve_date[5:7]), int(retrieve_date[8:]))
        b = date(int(rent_date[:4]), int(rent_date[5:7]), int(rent_date[8:]))

        day=(a-b).days

        if day>30:
            punishment=(day-30)*1

        else:
            punishment=0

            list=[]
            list.append(data[0][0])
            list.append(data[0][1])
            list.append(data[0][2])
            list.append(day)
            list.append(punishment)
            list=tuple(list)
            new_data=[]
            new_data.append(list)

            self.tableWidget_3.insertRow(0)
            for row, form in enumerate(new_data):
                for column, item in enumerate(form):
                    self.tableWidget_3.setItem(row, column, QtWidgets.QTableWidgetItem(str(item)))
                    column += 1

                row_position = self.tableWidget_3.rowCount()
                self.tableWidget_3.insertRow(row_position)

    def Main(self):
        self.MainWindow = QtWidgets.QMainWindow()
        self.ui=Ui_MainWindow()
        self.ui.setupUi(MainWindow)

import icons_rc


